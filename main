encoded_bytes = [
    0x21, 0x31, 0x26, 0x39, 0x73, 0x2c, 0x36, 0x72, 
    0x1d, 0x36, 0x2a, 0x71, 0x1d, 0x2f, 0x76, 0x73, 
    0x2c, 0x24, 0x30, 0x76, 0x2f, 0x71, 0x3f
]

# XOR each byte with 0x42
flag = ''.join(chr(b ^ 0x42) for b in encoded_bytes)
print(f"Access Code: {flag}")
print(f"Length: {len(flag)}")